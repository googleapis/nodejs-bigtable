{
  "name": "@google-cloud/bigtable",
  "description": "Cloud Bigtable Client Library for Node.js",
  "version": "1.0.1",
  "license": "Apache-2.0",
  "author": "Google Inc.",
  "engines": {
    "node": ">=6.0.0"
  },
  "repository": "googleapis/nodejs-bigtable",
  "main": "build/src/index.js",
  "files": [
    "build/protos",
    "build/proto",
    "build/src",
    "!**/*.d.ts",
    "!**/*.map"
  ],
  "keywords": [
    "google apis client",
    "google api client",
    "google apis",
    "google api",
    "google",
    "google cloud platform",
    "google cloud",
    "cloud",
    "google bigtable",
    "bigtable"
  ],
  "scripts": {
    "docs": "jsdoc -c .jsdoc.js",
    "samples-test": "cd samples/ && npm link ../ && npm test && cd ../",
    "test": "nyc mocha build/test",
    "generate-scaffolding": "repo-tools generate all && repo-tools generate lib_samples_readme -l samples/ --config ../.cloud-repo-tools.json",
    "lint": "eslint '**/*.js'",
    "presystem-test": "npm run compile",
    "system-test": "mocha build/system-test --timeout 600000",
    "snippet-test": "mocha samples/document-snippets/tests/*.js --timeout 600000",
    "fix": "eslint --fix '**/*.js'",
    "docs-test": "linkinator docs -r --skip www.googleapis.com",
    "predocs-test": "npm run docs",
    "clean": "gts clean",
    "compile": "tsc -p . && cp -r src/v2 build/src/ && cp -r proto* build/ && cp test/*.js build/test",
    "prepare": "npm run compile",
    "pretest": "npm run compile",
    "proto": "mkdir -p proto && pbjs -t static-module -w commonjs -p protos google/bigtable/v2/bigtable.proto | pbts -o proto/bigtable.d.ts -"
  },
  "dependencies": {
    "@google-cloud/common-grpc": "^0.10.0",
    "@google-cloud/paginator": "^0.1.0",
    "@google-cloud/projectify": "^0.3.0",
    "@google-cloud/promisify": "^0.4.0",
    "arrify": "^1.0.0",
    "concat-stream": "^2.0.0",
    "dot-prop": "^4.2.0",
    "escape-string-regexp": "^1.0.5",
    "extend": "^3.0.0",
    "google-auth-library": "^3.0.0",
    "google-gax": "^0.25.0",
    "is": "^3.0.1",
    "is-utf8": "^0.2.1",
    "lodash.merge": "^4.6.0",
    "lodash.snakecase": "^4.1.1",
    "long": "^4.0.0",
    "protobufjs": "^6.8.0",
    "pumpify": "^1.3.3",
    "retry-request": "^4.0.0",
    "stream-events": "^1.0.2",
    "through2": "^3.0.0"
  },
  "devDependencies": {
    "@google-cloud/nodejs-repo-tools": "^3.0.0",
    "@types/arrify": "^1.0.4",
    "@types/extend": "^3.0.1",
    "@types/is": "0.0.21",
    "@types/mocha": "^5.2.6",
    "@types/p-queue": "^3.0.0",
    "@types/proxyquire": "^1.3.28",
    "@types/sinon": "^7.0.7",
    "@types/through2": "^2.0.34",
    "@types/uuid": "^3.4.4",
    "codecov": "^3.0.0",
    "eslint": "^5.0.0",
    "eslint-config-prettier": "^4.0.0",
    "eslint-plugin-node": "^8.0.0",
    "eslint-plugin-prettier": "^3.0.0",
    "gts": "^0.9.0",
    "intelli-espower-loader": "^1.0.1",
    "jsdoc": "^3.5.5",
    "jsdoc-baseline": "git+https://github.com/hegemonic/jsdoc-baseline.git",
    "linkinator": "^1.1.2",
    "mocha": "^6.0.0",
    "nyc": "^13.0.0",
    "p-queue": "^3.0.0",
    "power-assert": "^1.4.4",
    "prettier": "^1.7.4",
    "proxyquire": "^2.0.0",
    "sinon": "^7.0.0",
    "source-map-support": "^0.5.10",
    "typescript": "~3.1.0",
    "uuid": "^3.0.1"
  }
}
